{
  "name": "usersdata",
  "version": "1.0.0",
  "description": "usersdata node repo",
  "main": "dist/src/index.js",
  "repository": {
    "type": "git"
  },
  "keywords": [
    "muS",
    "Template"
  ],
  "author": "Team NONE",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "~7.9.0",
    "@turf/turf": "~5.1.6",
    "body-parser": "~1.19.0",
    "bookshelf": "~0.10.4",
    "bookshelf-json-columns": "~2.1.1",
    "compression": "~1.7.4",
    "cors": "~2.8.5",
    "csvtojson": "~2.0.10",
    "express": "~4.17.1",
    "express-boom": "~1.0.0",
    "express-pino-logger": "~1.1.0",
    "joi": "~17.2.1",
    "knex": "~0.21.8",
    "lodash.clonedeep": "~4.5.0",
    "lodash.set": "~4.3.2",
    "mysql2": "~2.2.5"
  },
  "devDependencies": {
    "@types/bookshelf": "~0.9.1",
    "eslint": "^4.19.1",
    "eslint-config-pundits": "~1.0.0",
    "@types/chai": "~4.2.14",
    "@types/cors": "~2.8.8",
    "@types/express": "~4.17.8",
    "@types/express-boom": "~3.0.0",
    "@types/knex": "~0.0.50",
    "@types/mocha": "~8.0.3",
    "@types/node": "~13.13.26",
    "@types/sinon": "~2.3.7",
    "@types/sinon-chai": "~3.2.5",
    "source-map-support": "~0.5.12",
    "ts-mock-imports": "~1.2.3",
    "ts-sinon": "~1.0.17",
    "chai": "~4.2.0",
    "chai-http": "~4.3.0",
    "husky": "~0.14.3",
    "mocha": "^8.2.1",
    "mocha-junit-reporter": "~1.23.3",
    "nodemon": "~2.0.6",
    "nyc": "~15.1.0",
    "pino-pretty": "~3.0.1",
    "sinon": "^4.4.8",
    "supertest": "~4.0.2",
    "ts-node": "~9.0.0",
    "tslint": "~5.20.1",
    "typescript": "~4.0.3",
    "uuid": "~3.1.0"
  },
  "scripts": {
    "start": "npm run build && npm run serve",
    "build": "npm run clean && npm run build-ts",
    "clean": "rm -rf dist",
    "serve": "node dist/src/index.js",
    "start-pretty": "npm run build && npm run serve-pretty",
    "serve-pretty": "node dist/src/index.js | pino-pretty ",
    "build-ts": "tsc",
    "test-watch": "npm run ts-test -- --watch",
    "dev": "tsc --watch & nodemon --delay 1 --exec \"ts-node dist/index.js | pino-pretty\"",
    "debug": "tsc --watch & nodemon --inspect=0.0.0.0 -- dist/src/index.js",
    "ts-test": "mocha --config ./.mocharc.json --recursive --timeout 500000 test/appService.spec.ts 'test/*.spec.ts' 'test/**/*.spec.ts' --exit",
    "ts-coverage": "nyc npm run ts-test",
    "ts-test-watch": "mocha --config ./.mocharc.json --recursive test/appService.spec.ts 'test/*.spec.ts' 'test/**/*.spec.ts' --watch --watch-files src,test",
    "coverage-build-agent": "nyc mocha --config ./.mocharc.json --recursive --timeout 500000 test/appService.spec.ts 'test/**/*.spec.ts' 'test/*.spec.ts' --reporter spec --reporter mocha-junit-reporter --reporter-options mochaFile=./test-results/test-results.xml --exit"
  }
}